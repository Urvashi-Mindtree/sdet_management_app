
<h3> Courses Available
<table class="table">
  <tr>
    <th>Course Name</th>
	<th>Start Date</th>
	<th>End Date</th>
	<th>Add Trainee</th>
  </tr>

  <% @courses_list.each do |course|%>
  <tr>
    <td><%= course.course_name %></td> 
	 <td><%= course.start_date %></td>
	 <td><%= course.end_date %></td>
	 <td><%= link_to 'Click to add', new_trainee_path(:param1=>course.id, :start_date=>course.start_date,:end_date=>course.end_date)  %></td>
  </tr>
  <% end %>
</table>

